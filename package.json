{
  "name": "mini-browser",
  "version": "1.0.211",
  "description": "Mini browser for use for Programming performance",
  "main": "app/index.js",
  "scripts": {
    "start": "electron .",
    "mini-browser": "powershell -File execute.ps1",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build": "electron-packager . --icon assets/webview --overwrite --out=packages",
    "package-win": "electron-packager . mini-browser --platform=win32 --arch=x64 --out=dist --overwrite",
    "electron:package:mac": "yarn build && electron-builder -m -c.extraMetadata=build/electron.js",
    "electron:package:linux": "yarn build && electron-builder -l -c.extraMetadata=build/electron.js"
  },
  "author": "Matheus Souza",
  "email": "Jo√£o Matheus Freitas de Souza",
  "license": "MIT",
  "private": false,
  "build": {
    "appId": "com.mini_browser.app"
  },
  "devDependencies": {
    "@electron-forge/publisher-github": "^6.4.2",
    "@types/json-schema": "^7.0.13",
    "@types/node": "^20.6.3",
    "electron": "^26.2.2",
    "electron-builder": "^24.6.4",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "chokidar": "^3.5.3",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "json-schema-typed": "^8.0.1"
  }
}